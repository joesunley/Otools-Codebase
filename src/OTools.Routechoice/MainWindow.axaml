<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:common="clr-namespace:OTools.AvaCommon;assembly=OTools.AvaCommon"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:Class="OTools.Routechoice.MainWindow"
		Title="OTools.Routechoice">

	<Grid RowDefinitions="Auto,*,20">
		<Menu Grid.Row="0">
			<MenuItem Header="Yo" />
		</Menu>

		<Grid ColumnDefinitions="50,*" Grid.Row="1">
			
			<common:PaintBox x:Name="paintBox" Grid.Column="1" />
		</Grid>

		<StackPanel Orientation="Horizontal" Grid.Row="2">
			
		</StackPanel>
	</Grid>
	
</Window>
